# czy dwie pary drzwi prowadzą do tego samego pokoju
doorsInTheSameRoom(d1, d2)  <=>  exists r in Room:
   doorsInRoom(d1, r) and doorsInRoom(d2, r);



success_msg "mamy kuchnię z dwojgiem drzwi"
failure_msg "brak dwojga drzwi w kuchni"

exists x in Room : exists d1, d2 in Door:  
doorsInRoom(d1,x) and  d1!=d2 and doorsInTheSameRoom(d1, d2)
and type(x)="Kitchen" ;
